<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".App.Activity.PartsChangeActivity_BLU"
    android:focusable="true"
    android:focusableInTouchMode="true"
    >

    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@drawable/text_border_title"
        android:gravity="center_vertical"
        android:text="Parts Change(BLU)"
        android:textColor="#FFFF"
        android:textSize="30dp"
        android:textStyle="bold" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:background="@drawable/text_border_white_back_color"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tvStatus"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_weight="0"
            android:gravity="fill"
            android:text="진행사항 안내"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="17dp" />
    </LinearLayout>

    <Space
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_weight="0" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tvInformation"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@drawable/text_border_subtitle"
                android:gravity="center_vertical"
                android:text="[기본정보]"
                android:textColor="#FFFFFF"
                android:textSize="15dp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:background="@drawable/text_border_white_back_color"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvWorker"
                    android:layout_width="150dp"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_blue"
                    android:gravity="fill"
                    android:text="작업자"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textSize="17dp" />

                <EditText
                    android:id="@+id/etWorker"
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_lightblue"
                    android:enabled="false"
                    android:inputType="text"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textColorHint="#c9c9c9"
                    android:textSize="17dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/text_border_white_back_color"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvModelCode"
                    android:layout_width="150dp"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_blue"
                    android:gravity="fill"
                    android:text="Model Code"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textSize="17dp" />

                <EditText
                    android:id="@+id/etModelCode"
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_lightblue"
                    android:enabled="false"
                    android:inputType="text"
                    android:selectAllOnFocus="true"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textColorHint="#c9c9c9"
                    android:textSize="17dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/text_border_white_back_color"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvModelName"
                    android:layout_width="150dp"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_blue"
                    android:gravity="fill"
                    android:text="Model Name"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textSize="17dp" />

                <EditText
                    android:id="@+id/etModelName"
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_lightblue"
                    android:enabled="false"
                    android:inputType="text"
                    android:selectAllOnFocus="true"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textColorHint="#c9c9c9"
                    android:textSize="17dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:background="@drawable/text_border_white_back_color"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvSMTLine"
                    android:layout_width="150dp"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_blue"
                    android:gravity="fill"
                    android:text="SMT Line"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textSize="17dp" />

                <EditText
                    android:id="@+id/etSMTLine"
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_lightblue"
                    android:enabled="false"
                    android:inputType="text"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textColorHint="#c9c9c9"
                    android:textSize="17dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:background="@drawable/text_border_white_back_color"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvMarking"
                    android:layout_width="150dp"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_blue"
                    android:gravity="fill"
                    android:text="Marking or Lot No."
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textSize="17dp" />

                <EditText
                    android:id="@+id/etMarking"
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_lightblue"
                    android:enabled="true"
                    android:inputType="text"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textColorHint="#c9c9c9"
                    android:textSize="17dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:background="@drawable/text_border_lightblue"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvPartDivison"
                    android:layout_width="150dp"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_blue"
                    android:gravity="fill"
                    android:text="자재구분"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textSize="17dp" />

                <Spinner
                    android:id="@+id/spnPartDivison"
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:spinnerMode="dialog"
                    android:textAlignment="center"
                    android:textSize="17dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:background="@drawable/text_border_white_back_color"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvFeederSN"
                    android:layout_width="150dp"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_blue"
                    android:gravity="fill"
                    android:text="Feeder SN"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textSize="17dp" />

                <EditText
                    android:id="@+id/etFeederSN"
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_lightblue"
                    android:enabled="false"
                    android:inputType="text"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textColorHint="#c9c9c9"
                    android:textSize="17dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:orientation="horizontal">

                <CheckBox
                    android:id="@+id/noMarking"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Marking(Lot No.) X" />

                <CheckBox
                    android:id="@+id/firstParts"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Feeder Setting시 클릭" />
            </LinearLayout>

            <Space
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0" />

            <TextView
                android:id="@+id/tvInformation2"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@drawable/text_border_subtitle"
                android:gravity="center_vertical"
                android:text="[교환 전 자재]"
                android:textColor="#FFFFFF"
                android:textSize="15dp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:background="@drawable/text_border_white_back_color"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvBefPartNo"
                    android:layout_width="150dp"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_blue"
                    android:gravity="fill"
                    android:text="Part No."
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textSize="17dp" />

                <EditText
                    android:id="@+id/etBefPartNo"
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_lightblue"
                    android:enabled="false"
                    android:inputType="text"
                    android:selectAllOnFocus="true"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textColorHint="#c9c9c9"
                    android:textSize="17dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:background="@drawable/text_border_white_back_color"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvBefRank"
                    android:layout_width="150dp"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_blue"
                    android:gravity="fill"
                    android:text="Rank"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textSize="17dp" />

                <EditText
                    android:id="@+id/etBefRank"
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_lightblue"
                    android:enabled="false"
                    android:inputType="text"
                    android:selectAllOnFocus="true"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textColorHint="#c9c9c9"
                    android:textSize="17dp" />
            </LinearLayout>

            <Space
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:layout_weight="0" />

            <TextView
                android:id="@+id/tvInformation3"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_weight="0"
                android:background="@drawable/text_border_subtitle"
                android:gravity="center_vertical"
                android:text="[교환 후 자재]"
                android:textColor="#FFFFFF"
                android:textSize="15dp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:background="@drawable/text_border_white_back_color"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvAftPartNo"
                    android:layout_width="150dp"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_blue"
                    android:gravity="fill"
                    android:text="Part No."
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textSize="17dp" />

                <EditText
                    android:id="@+id/etAftPartNo"
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_lightblue"
                    android:enabled="false"
                    android:inputType="text"
                    android:selectAllOnFocus="true"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textColorHint="#c9c9c9"
                    android:textSize="17dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:background="@drawable/text_border_white_back_color"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvAftRank"
                    android:layout_width="150dp"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_blue"
                    android:gravity="fill"
                    android:text="Rank"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textSize="17dp" />

                <EditText
                    android:id="@+id/etAftRank"
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_lightblue"
                    android:enabled="false"
                    android:inputType="text"
                    android:selectAllOnFocus="true"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textColorHint="#c9c9c9"
                    android:textSize="17dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:background="@drawable/text_border_white_back_color"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvAftLotNo"
                    android:layout_width="150dp"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_blue"
                    android:gravity="fill"
                    android:text="Lot No."
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textSize="17dp" />

                <EditText
                    android:id="@+id/etAftLotNo"
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_lightblue"
                    android:enabled="true"
                    android:inputType="text"
                    android:selectAllOnFocus="true"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textColorHint="#c9c9c9"
                    android:textSize="17dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:background="@drawable/text_border_white_back_color"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvAftQty"
                    android:layout_width="150dp"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_blue"
                    android:gravity="fill"
                    android:text="Qty"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textSize="17dp" />

                <EditText
                    android:id="@+id/etAftQty"
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:layout_weight="0"
                    android:background="@drawable/text_border_lightblue"
                    android:enabled="true"
                    android:inputType="number"
                    android:selectAllOnFocus="true"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textColorHint="#c9c9c9"
                    android:textSize="17dp" />
            </LinearLayout>

            <Space
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:layout_weight="0" />
        </LinearLayout>
    </ScrollView>
</LinearLayout>